type Query {
    courses(filter: CourseFilterInput, sortKey: SortKeyInput): [CourseCatalogResp]
}

input CourseFilterInput {
    minPrice: Float
    maxPrice: Float
    location: String
    subject: String
}

input SortKeyInput {
    field: String
    order: String
}

type CourseCatalogResp {
    courseId: ID
    courseName: String
    tutorName: String
    tutorId: ID
    hourlyRate: Float
    location: String
    subject: String
    description: String
}
